<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="448.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hu.unideb.progtech.headswitcher.controller.HighScoreController">
	<children>
		<TableView fx:id="highScoreTable" prefHeight="400.0" prefWidth="350.0">
			<columns>
				<TableColumn fx:id="usernameCol" prefWidth="100.0" text="Username" />
				<TableColumn fx:id="goldCol" prefWidth="100.0" text="Gold" />
				<TableColumn fx:id="dateCol" minWidth="20.0" prefWidth="149.0" text="Date" />
			</columns>
		</TableView>
      <Button fx:id="backButton" layoutX="145.0" layoutY="400.0" mnemonicParsing="false" onAction="#handleBack" text="Back">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
	</children>
</AnchorPane>
